@model MyAWebpp.Models.Register
<h2>Create User here...</h2>
<br />
<form asp-area="" asp-action="Register" asp-controller="Account">
	<div class="form-group">
		<input asp-for="FirstName" class="form-control m-1" placeholder="Enter First Name" />
	</div>
	<div class="form-group">
		<input asp-for="LastName" class="form-control m-1" placeholder="Enter Last Name" />
	</div>
	<div class="form-group">
		<input asp-for="Email" class="form-control m-1" placeholder="Enter Email" />
	</div>
	<div class="form-group">
		<input asp-for="Phone" class="form-control m-1" placeholder="Enter Pnone No." />
	</div>
	<div>
		<input type="password" asp-for="Password" class="form-control m-1" placeholder="Enter password" />
	</div>
	<br />
	<div class="form-group">
		<button type="submit" class="btn btn-primary">Create</button>
	</div>
</form>